-H "Content-Type: application/json" \
-d '{
    "name": "awdawd"
}'

curl -X POST http://localhost:8080/product/create-table

curl -X POST http://localhost:8080/product/insert \
-H "Content-Type: application/json" \
-d '{
    "Name": "<string>",
    "ProductionTime": <int>
}'
